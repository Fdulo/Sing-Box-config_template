{
  "log": {
    "disabled": true,
    "level": "warn",
    "timestamp": true
  },
  "experimental": {
    "cache_file": {
      "enabled": false,
      "store_fakeip": true
    },
    "clash_api": {
      "external_controller": "127.0.0.1:9095",
      "external_ui": "ui",
      "secret": "zxcv...Q",
      "external_ui_download_url": "https://ghfast.top/https://github.com/MetaCubeX/metacubexd/archive/refs/heads/gh-pages.zip",
      "external_ui_download_detour": "direct-out",
      "default_mode": "rule"
    }
  },
  "dns": {
   "fakeip": {
            "enabled": true,
            "inet4_range": "198.18.0.0/15",
            "inet6_range": "fc00:8964::/32"
        },
        "servers": [
            {
                "tag": "dns_proxy",
                "address": "https://1.1.1.1/dns-query",
                "address_resolver": "dns_first",
                "detour": "proxy"
            },
            {
                "tag": "dns_direct",
                "address": "223.6.6.6",
                "address_resolver": "dns_first",
                "detour": "direct-out"
            },
            {
                "tag": "dns_first",
                "address": "223.6.6.6",
                "detour": "direct-out"
            },
            {
                "tag": "fakeip",
                "address": "fakeip"
            }
    ],
    "rules": [
      {
          "outbound": "any",
          "server": "dns_direct"
       },
      {
        "action": "route",
        "clash_mode": "Direct",
        "server": "dns_direct"
      },
      {
        "action": "route",
        "clash_mode": "Global",
        "server": "fakeip"
      },
      {
        "rule_set": "geosite-cn",
        "server": "dns_direct",
        "rewrite_ttl": 43200
      },
      {
          "query_type": [
              "A",
              "AAAA"
          ],
          "server": "fakeip",
          "rewrite_ttl": 1,
          "disable_cache": true
      }
    ],
    "final": "dns_proxy",
    "independent_cache": true
  },
  "inbounds": [
    {
      "tag": "tun-in",
      "type": "tun",
      "address": [
        "172.19.0.1/30",
        "fdfe:2952:8964::1/126"
      ],
      "stack": "gvisor",
      "auto_route": true
    }
  ],
    "outbounds": [
    {
      "tag": "direct-out",
      "type": "direct"
    },
    {
      "tag": "proxy",
      "type": "selector",
      "outbounds": [
        "general-detour",
        "direct-out"
      ],
      "default": "general-detour"
    },
    {
      "tag": "AI",
      "type": "selector",
      "outbounds": [
        "proxy",
        "direct-out",
        "alternative-route"
      ],
      "default": "proxy"
    },
    {
      "tag": "YouTube",
      "type": "selector",
      "outbounds": [
        "proxy",
        "direct-out",
        "alternative-route"
      ],
      "default": "proxy"
    },
    {
      "tag": "Google",
      "type": "selector",
      "outbounds": [
        "proxy",
        "direct-out",
        "alternative-route"
      ],
      "default": "proxy"
    },
    {
      "tag": "telegram",
      "type": "selector",
      "outbounds": [
        "proxy",
        "direct-out",
        "alternative-route"
      ],
      "default": "proxy"
    },
    {
      "tag": "general-detour",
      "type": "selector",
      "outbounds": [
            "{all}"
      ]
    },
    {
      "tag": "alternative-route",
      "type": "selector",
      "outbounds": [
           "{all}"
      ]
    },
    {
      "tag": "final",
      "type": "selector",
      "outbounds": [
        "proxy",
        "direct-out"
      ],
      "default": "proxy"
    }
  ],
  "route": {
    "auto_detect_interface": true,
    "final": "proxy"
    "rules": [
      {
      "type": "logical",
      "mode": "or",
      "rules": [
        {"port": 853},
        {"network":"udp","port":443},
        {"protocol":"stun"}
      ],
      "action": "reject"
     },
     {
        "inbound": "tun-in",
        "action": "resolve",
        "strategy": "prefer_ipv4"
      },
     {
        "inbound": "tun-in",
        "action": "sniff",
        "timeout": "1s"
      },
      {
        "type": "logical",
        "mode": "or",
        "rules": [
          {"protocol": "dns"},
          {"network": "udp","port": 53}
        ],
        "action": "hijack-dns"
      },
      {
        "clash_mode": "Direct",
        "outbound": "direct-out"
      },
      {
        "ip_is_private": true,
        "outbound": "direct-out"
      },
      {
        "clash_mode": "Global",
        "outbound": "proxy"
      },
      {
        "rule_set": [
          "ai_non_ip"
        ],
        "outbound": "AI"
      },
        {
        "rule_set": [
          "geosite-youtube"
        ],
        "outbound": "YouTube"
      },
      {
        "rule_set": [
          "geoip-google",
          "geosite-google"
        ],
        "outbound": "Google"
      },
      {
        "rule_set": [
          "telegram_non_ip"
        ],
        "outbound": "telegram"
      },
      {
        "rule_set": [
          "telegram_ip"
        ],
        "outbound": "telegram"
      },
      {
        "rule_set": [
          "geosite:cn"
        ],
        "outbound": "direct_out"
      },
      {
        "rule_set": [
          "geoip:cn"
        ],
        "outbound": "direct_out"
      }
    ],
    "rule_set": [
      {
        "tag": "ai_non_ip",
        "type": "remote",
        "format": "binary",
        "url": "https://ghfast.top/https://github.com/kunori-kiku/sing-box-ruleset/raw/main/non_ip/ai.srs",
        "download_detour": "direct-out"
      },
      {
        "tag": "telegram_non_ip",
        "type": "remote",
        "format": "binary",
        "url": "https://ghfast.top/https://github.com/kunori-kiku/sing-box-ruleset/raw/main/non_ip/telegram.srs",
        "download_detour": "direct-out"
      },
      {
        "tag": "telegram_ip",
        "type": "remote",
        "format": "binary",
        "url": "https://ghfast.top/https://github.com/kunori-kiku/sing-box-ruleset/raw/main/ip/telegram.srs",
        "download_detour": "direct-out"
      },
      {
        "tag": "geosite-youtube",
        "type": "remote",
        "format": "binary",
        "url": "https://ghfast.top/https://raw.githubusercontent.com/MetaCubeX/meta-rules-dat/sing/geo/geosite/youtube.srs",
        "download_detour": "direct-out"
      },
      {
        "tag": "geoip-google",
        "type": "remote",
        "format": "binary",
        "url": "https://ghfast.top/https://raw.githubusercontent.com/MetaCubeX/meta-rules-dat/sing/geo/geoip/google.srs",
        "download_detour": "direct-out"
      },
      {
        "tag": "geosite-google",
        "type": "remote",
        "format": "binary",
        "url": "https://ghfast.top/https://raw.githubusercontent.com/MetaCubeX/meta-rules-dat/sing/geo/geosite/google.srs",
        "download_detour": "direct-out"
      },
      {
        "tag": "geoip-cn",
        "type": "remote",
        "format": "binary",
        "url": "https://ghfast.top/https://github.com/MetaCubeX/meta-rules-dat/raw/sing/geo/geoip/cn.srs",
        "download_detour": "direct-out"
      },
      {
        "tag": "geosite-cn",
        "type": "remote",
        "format": "binary",
        "url": "https://ghfast.top/https://github.com/MetaCubeX/meta-rules-dat/raw/sing/geo/geosite/cn.srs",
        "download_detour": "direct-out"
      }
    ]
  }
}
