{
    "log": {
        "disabled": false,
        "level": "warn",
        "timestamp": true
    },
    "experimental": {
        "cache_file": {
            "enabled": true,
            "store_fakeip": true,
            "store_rdrc": false
        },
        "clash_api": {
            "external_controller": "127.0.0.1:9090",
            "external_ui": "dashboard",
            "external_ui_download_url": "https://ghfast.top/https://github.com/Zephyruso/zashboard/releases/latest/download/dist.zip",
            "external_ui_download_detour": "direct-out",
            "default_mode": "rule"
        }
    },
    "dns": {
        "fakeip": {
            "enabled": true,
            "inet4_range": "198.18.0.0/15",
            "inet6_range": "fc00:8964::/32"
        },
        "servers": [
            {
                "tag": "dns_proxy",
                "address": "https://8.8.8.8/dns-query",
                "address_resolver": "dns_direct",
                "address_strategy": "ipv4_only",
                "detour": "proxy"
            },
            {
                "tag": "dns_direct",
                "address": "https://223.5.5.5/dns-query",
                "address_resolver": "dns-local",
                "address_strategy": "ipv4_only",
                "detour": "direct-out"
            },
            {
                "tag": "dns-local",
                "address": "local",
                "detour": "direct-out"
            },
            {
                "tag": "fakeip",
                "address_strategy": "ipv4_only",
                "address": "fakeip"
            }
        ],
        "rules": [
            {
                "outbound": "any",
                "server": "dns_direct"
            },
            {
                "action": "route",
                "clash_mode": "direct",
                "server": "dns_direct"
            },
            {
                "action": "route",
                "clash_mode": "global",
                "server": "fakeip"
            },
            {
                "action": "route",
                "query_type": [
                    "A",
                    "AAAA"
                ],
                "server": "fakeip",
                "rewrite_ttl": 1
            },
            {
                "type": "logical",
                "mode": "and",
                "rules": [
                    {
                        "rule_set": "geolocation-!cn",
                        "invert": true
                    },
                    {
                        "rule_set": "geoip-cn"
                    }
                ],
                "server": "dns_proxy",
                "client_subnet": "61.141.249.0/24"
            }
        ],
        "independent_cache": true,
        "final": "dns_proxy"
    },
    "inbounds": [
        {
            "tag": "tun-in",
            "type": "tun",
            "interface_name": "singbox",
            "address": [
                "172.19.0.1/30",
                "fdfe:2952:8964::1/126"
            ],
            "stack": "mixed",
            "auto_route": true,
            "strict_route": true,
            "platform": {
        "http_proxy": {
          "enabled": true,
          "server": "127.0.0.1",
          "server_port": 7890
         }
