      "tag": "general-detour",
      "type": "selector",
      "outbounds": [
            "{all}"
      ],
    "interrupt_exist_connections": true
    },
    {
      "tag": "alternative-route",
      "type": "selector",
      "outbounds": [
            "{all}"
      ],
    "interrupt_exist_connections": true
    },
    {
            "tag": "final",
            "type": "selector",
            "outbounds": [
                "proxy",
                "direct-out"
            ],
            "default": "proxy"
      }
