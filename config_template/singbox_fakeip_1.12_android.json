{
  "log": {
    "disabled": false,
    "level": "warn",
    "timestamp": true
  },
  "experimental": {
    "cache_file": {
      "enabled": true,
      "store_fakeip": false,
      "store_rdrc": false
    },
    "clash_api": {
      "external_controller": "127.0.0.1:9095",
      "external_ui": "ui",
      "secret": "zxcv...Q",
      "external_ui_download_url": "https://ghfast.top/https://github.com/MetaCubeX/metacubexd/archive/refs/heads/gh-pages.zip",
      "external_ui_download_detour": "direct-out",
      "default_mode": "rule"
      }
  },
  "dns": {
    "servers": [
      {
        "tag": "dns_proxy",
        "type": "https",             
        "server": "8.8.8.8",
        "domain_resolver": "dns_direct",
        "detour": "proxy"
      },
      {
        "tag": "dns_direct",
        "type": "https",             
        "server": "223.5.5.5",
        "domain_resolver": "dhcp"
      },
      {
      "type": "local",
      "tag": "dhcp"
      },
     {
        "tag": "fakeip",
        "type": "fakeip",          
        "inet4_range": "198.18.0.0/15",
        "inet6_range": "fc00::/18"  
      }
    ],
    "rules": [
      {
        "action": "route",
        "clash_mode": "Direct",
        "server": "dns_direct"
      },
      {
        "action": "route",
        "clash_mode": "Global",
        "server": "fakeip"
      },
      {
        "query_type": [
          "A",
          "AAAA"
        ],
        "rule_set": "geosite-cn",
        "server": "dns_direct"
      },
      {
        "query_type": [
          "CNAME"
        ],
        "rule_set": "geosite-cn",
        "server": "dns_direct"
      },
      {
        "type": "logical",
        "mode": "and",
        "rules": [
          {
            "rule_set": "geolocation-!cn",
            "invert": true
          },
          {
            "rule_set": "geoip-cn"
          }
        ],
        "server": "dns_proxy"
      },
      {
        "query_type": [
          "A",
          "AAAA"
        ],
        "server": "fakeip"
      },
      {
        "query_type": [
          "CNAME"
        ],
        "server": "dns_proxy"
      }
    ],
        "final": "dns_proxy",
        "disable_cache": false,
        "disable_expire": false,
        "independent_cache": true,
        "reverse_mapping": false
  },
  "inbounds": [
    {
      "tag": "tun-in",
      "type": "tun",
      "address": [
        "172.19.0.1/30",
        "fdfe:2952:8964::1/126"
      ],
      "stack": "system",
      "auto_route": true,
      "strict_route": true
    }
  ],
  "outbounds": [
    {
      "tag": "direct-out",
      "type": "direct"
    },
    {
      "tag": "proxy",
      "type": "selector",
      "outbounds": [
        "general-detour",
        "direct-out"
      ],
      "default": "general-detour"
    },
    {
      "tag": "openai",
      "type": "selector",
      "outbounds": [
        "proxy",
        "direct-out",
        "alternative-route",
        "final"
      ],
      "default": "proxy"
    },
    {
      "tag": "YouTube",
      "type": "selector",
      "outbounds": [
        "proxy",
        "direct-out",
        "alternative-route",
        "final"
      ],
      "default": "proxy"
    },
    {
      "tag": "Google",
      "type": "selector",
      "outbounds": [
        "proxy",
        "direct-out",
        "alternative-route",
        "final"
      ],
      "default": "proxy"
    },
    {
       "tag":"Spotify",
       "type":"selector",
       "outbounds":[
        "proxy",
        "direct-out",
        "alternative-route",
        "final"
       ],
       "default": "proxy"
    },
    {
      "tag": "telegram",
      "type": "selector",
      "outbounds": [
        "proxy",
        "direct-out",
        "alternative-route",
        "final"
      ],
      "default": "proxy"
    },
    {
      "tag": "microsoft",
      "type": "selector",
      "outbounds": [
        "proxy",
        "direct-out",
        "alternative-route",
        "final"
      ],
      "default": "proxy"
    },
    {
      "tag": "general-detour",
      "type": "selector",
      "outbounds": [
            "{all}"
      ]
    },
    {
      "tag": "alternative-route",
      "type": "selector",
      "outbounds": [
            "{all}"
      ]
    },
    {
      "tag": "final",
      "type": "selector",
      "outbounds": [
        "proxy",
        "direct-out"
      ],
      "default": "proxy"
    }
],
  "route": {
"default_domain_resolver": {
        "server": "dns_direct"
      },
    "rules": [
            {
        "action": "sniff"
            },
            {
        "type": "logical",
        "mode": "or",
        "rules": [
          {
            "protocol": "dns"
          },
          {
            "network": "udp",
            "port": 53
          }
        ],
        "action": "hijack-dns"
      },
      {
        "clash_mode": "Direct",
        "action": "route",
        "outbound": "direct-out"
      },
      { "ip_is_private": true, "outbound": "direct-out" },
      {
                "type": "logical",
                "mode": "or",
                "rules": [{"port":853},{"network":"udp","port":443},{"protocol":"stun"}],
                "action": "reject",
                "method": "default",
                "no_drop": false
      },
      {
        "clash_mode": "Global",
        "action": "route",
        "outbound": "proxy"
      },
      {
        "action": "route",
        "rule_set": [
          "ai_non_ip"
        ],
        "outbound": "openai"
      },
      {
        "action": "route",
        "rule_set": [
          "telegram_non_ip"
        ],
        "outbound": "telegram"
      },
      {
        "action": "route",
        "rule_set": [
          "microsoft_cdn_non_ip"
        ],
        "outbound": "direct-out"
      },
      {
        "action": "route",
        "rule_set": [
          "microsoft_non_ip"
        ],
        "outbound": "microsoft"
      },
      {
        "action": "route",
        "rule_set": [
          "geosite-youtube"
        ],
        "outbound": "YouTube"
      },
      {
        "action": "route",
        "package_name": [
        "app.rvx.android.youtube"
       ],
        "outbound": "YouTube"
      },
      {
        "action": "route",
        "rule_set": [
          "geoip-google",
          "geosite-google"
        ],
        "outbound": "Google"
      },
      {
        "rule_set": [
          "telegram_ip"
        ],
        "action": "route",
        "outbound": "telegram"
      },
      {
        "rule_set": [
          "geosite-spotify"
        ],
        "action": "route",
        "outbound": "Spotify"
      },
      {
        "action": "route",
        "rule_set": "geolocation-!cn",
        "outbound": "proxy"
      },
      {
        "rule_set": [
          "geosite-cn", 
          "geoip-cn"          
        ],
        "action": "route",
        "outbound": "direct-out"
      }
    ],
    "rule_set": [
