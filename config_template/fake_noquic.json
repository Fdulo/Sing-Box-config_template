{
  "log": {
    "disabled": false,
    "level": "warn",
    "timestamp": true
  },
  "experimental": {
    "cache_file": {
      "enabled": true,
      "store_fakeip": true,
      "store_rdrc": false
    },
    "clash_api": {
      "external_controller": "127.0.0.1:9095",
      "external_ui": "ui",
      "secret": "zxcv...Q",
      "external_ui_download_url": "https://ghfast.top/https://github.com/MetaCubeX/metacubexd/archive/refs/heads/gh-pages.zip",
      "external_ui_download_detour": "direct-out",
      "default_mode": "rule"
    }
  },
  "dns": {
    "servers": [
      {
        "tag": "dns_proxy",
        "type": "https",
        "server": "1.1.1.1",
        "domain_resolver": "dns_direct",
        "detour": "proxy"
      },
      {
        "tag": "dns_direct",
        "type": "udp",
        "server": "223.6.6.6",
        "domain_resolver": "local",
        "detour": "direct-out"
      },
      {
        "tag": "local",
        "type": "local"
      },
      {
        "tag": "fakeip",
        "type": "fakeip",
        "inet4_range": "198.18.0.0/15",
        "inet6_range": "fc00::/18"
      }
    ],
    "rules": [
      {
        "action": "route",
        "clash_mode": "Direct",
        "server": "dns_direct"
      },
      {
        "action": "route",
        "clash_mode": "Global",
        "server": "fakeip"
      },
      {
          "query_type": [
              "A",
              "AAAA"
          ],
          "rule_set": "geosite-cn",
          "invert": true,
          "server": "fakeip",
          "rewrite_ttl": 1
      },
      {
        "rule_set": "geosite-cn",
        "invert": true,
        "server": "dns_proxy"
      },
      {
        "rule_set": "geosite-cn",
        "server": "dns_direct"
      }
    ],
    "final": "dns_proxy",
    "independent_cache": true
  },
  "inbounds": [
    {
      "tag": "tun-in",
      "type": "tun",
      "address": [
        "172.19.0.1/30",
        "fdfe:2952:8964::1/126"
      ],
      "mtu": 1450,
      "stack": "system",
      "auto_route": true
    }
  ],
    "outbounds": [
    {
      "tag": "direct-out",
      "type": "direct"
    },
    {
      "tag": "proxy",
      "type": "selector",
      "outbounds": [
        "general-detour",
        "direct-out"
      ],
      "default": "general-detour"
    },
    {
      "tag": "openai",
      "type": "selector",
      "outbounds": [
        "proxy",
        "direct-out",
        "alternative-route",
        "final"
      ],
      "default": "proxy"
    },
    {
      "tag": "YouTube",
      "type": "selector",
      "outbounds": [
        "proxy",
        "direct-out",
        "alternative-route"
      ],
      "default": "proxy"
    },
    {
      "tag": "Google",
      "type": "selector",
      "outbounds": [
        "proxy",
        "direct-out",
        "alternative-route"
      ],
      "default": "proxy"
    },
    {
       "tag":"Spotify",
       "type":"selector",
       "outbounds":[
        "proxy",
        "direct-out",
        "alternative-route"
       ],
       "default": "proxy"
    },
    {
      "tag": "telegram",
      "type": "selector",
      "outbounds": [
        "proxy",
        "direct-out",
        "alternative-route"
      ],
      "default": "proxy"
    },
    {
      "tag": "microsoft",
      "type": "selector",
      "outbounds": [
        "proxy",
        "direct-out",
        "alternative-route"
      ],
      "default": "proxy"
    },
    {
      "tag": "general-detour",
      "type": "selector",
      "outbounds": [
            "{all}"
      ]
    },
    {
      "tag": "alternative-route",
      "type": "selector",
      "outbounds": [
           "{all}"
      ]
    },
    {
      "tag": "final",
      "type": "selector",
      "outbounds": [
        "proxy",
        "direct-out"
      ],
      "default": "proxy"
    }
  ],
  "route": {
    "auto_detect_interface": true,
    "override_android_vpn": true,
    "default_domain_resolver": "dns_direct",
    "rules": [
      {
      "type": "logical",
      "mode": "or",
      "rules": [
        {"port": 853},
        {"network":"udp","port":443},
        {"protocol":"stun"}
      ],
      "action": "reject"
      },
      {
        "sniffer": ["http", "tls"],
        "action": "sniff",
        "timeout": "1s"
      },
      {
        "type": "logical",
        "mode": "or",
        "rules": [
          {"protocol": "dns"},
          {"network": "udp","port": 53}
        ],
        "action": "hijack-dns"
      },
      {
        "clash_mode": "Direct",
        "outbound": "direct-out"
      },
      {
        "ip_is_private": true,
        "outbound": "direct-out"
      }, 
      {
        "clash_mode": "Global",
        "outbound": "proxy"
      },
      {
        "rule_set": [
          "ai_non_ip"
        ],
        "outbound": "openai"
      },
      {
        "rule_set": [
          "telegram_non_ip"
        ],
        "outbound": "telegram"
      },
      {
        "rule_set": [
          "microsoft_cdn_non_ip"
        ],
        "outbound": "direct-out"
      },
      {
        "rule_set": [
          "microsoft_non_ip"
        ],
        "outbound": "microsoft"
      },
      {
        "rule_set": [
          "geosite-youtube"
        ],
        "outbound": "YouTube"
      },
      {
        "package_name": [
        "app.rvx.android.youtube"
       ],
        "outbound": "YouTube"
      },
      {
        "rule_set": [
          "geoip-google",
          "geosite-google"
        ],
        "outbound": "Google"
      },
      {
        "rule_set": [
          "telegram_ip"
        ],
        "outbound": "telegram"
      },
      {
        "rule_set": [
          "geosite-spotify"
        ],
        "outbound": "Spotify"
      },
      {
        "rule_set": [
          "geosite-cn",
          "geoip-cn" 
        ],
        "outbound": "direct-out"
      },
      {
        "rule_set": "geolocation-!cn",
        "outbound": "proxy"
      }
    ],
    "rule_set": [
      {
        "tag": "ai_non_ip",
        "type": "remote",
        "format": "binary",
        "url": "https://ghfast.top/https://github.com/kunori-kiku/sing-box-ruleset/raw/main/non_ip/ai.srs",
        "download_detour": "direct-out"
      },
      {
        "tag": "telegram_non_ip",
        "type": "remote",
        "format": "binary",
        "url": "https://ghfast.top/https://github.com/kunori-kiku/sing-box-ruleset/raw/main/non_ip/telegram.srs",
        "download_detour": "direct-out"
      },
      {
        "tag": "telegram_ip",
        "type": "remote",
        "format": "binary",
        "url": "https://ghfast.top/https://github.com/kunori-kiku/sing-box-ruleset/raw/main/ip/telegram.srs",
        "download_detour": "direct-out"
      },
      {
        "tag": "microsoft_cdn_non_ip",
        "type": "remote",
        "format": "binary",
        "url": "https://ghfast.top/https://github.com/kunori-kiku/sing-box-ruleset/raw/main/non_ip/microsoft_cdn.srs",
        "download_detour": "direct-out"
      },
      {
        "tag": "microsoft_non_ip",
        "type": "remote",
        "format": "binary",
        "url": "https://ghfast.top/https://github.com/kunori-kiku/sing-box-ruleset/raw/main/non_ip/microsoft.srs",
        "download_detour": "direct-out"
      },
      {
        "tag": "geosite-youtube",
        "type": "remote",
        "format": "binary",
        "url": "https://ghfast.top/https://raw.githubusercontent.com/MetaCubeX/meta-rules-dat/sing/geo/geosite/youtube.srs",
        "download_detour": "direct-out"
      },
      {
        "tag": "geoip-google",
        "type": "remote",
        "format": "binary",
        "url": "https://ghfast.top/https://raw.githubusercontent.com/MetaCubeX/meta-rules-dat/sing/geo/geoip/google.srs",
        "download_detour": "direct-out"
      },
      {
        "tag": "geosite-google",
        "type": "remote",
        "format": "binary",
        "url": "https://ghfast.top/https://raw.githubusercontent.com/MetaCubeX/meta-rules-dat/sing/geo/geosite/google.srs",
        "download_detour": "direct-out"
      },
      {
        "tag": "geosite-spotify",
        "type": "remote",
        "format": "binary",
        "url": "https://ghfast.top/https://raw.githubusercontent.com/MetaCubeX/meta-rules-dat/sing/geo/geosite/spotify.srs",
        "download_detour": "direct-out"
      },
      {
        "tag": "geolocation-!cn",
        "type": "remote",
        "format": "binary",
        "url": "https://ghfast.top/https://github.com/MetaCubeX/meta-rules-dat/raw/sing/geo/geosite/geolocation-!cn.srs",
        "download_detour": "direct-out"
      },
      {
        "tag": "geoip-cn",
        "type": "remote",
        "format": "binary",
        "url": "https://ghfast.top/https://github.com/MetaCubeX/meta-rules-dat/raw/sing/geo/geoip/cn.srs",
        "download_detour": "direct-out"
      },
      {
        "tag": "geosite-cn",
        "type": "remote",
        "format": "binary",
        "url": "https://ghfast.top/https://github.com/MetaCubeX/meta-rules-dat/raw/sing/geo/geosite/cn.srs",
        "download_detour": "direct-out"
      }
    ],
    "final": "final"
  }
}
